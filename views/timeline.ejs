<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <title><%= title?title:bag %></title>
    <link href="/static/bootstrap.min.css" media="all" rel="stylesheet">
    <link href="/static/styles.css" media="all" rel="stylesheet">
</head>

<body id="body">
    <span id="topAnchor"></span>
    <a class="home" href="/">首页</a>
    <a class="top" href="#topAnchor">顶部</a>
    <h1 class="htitle"><%= title?title:bag %></h1>
    <h2 class="htitle"=><%= bag %></h2>
    <div class="timeline animated">
        <% for(let i=0;i<data.length;i++){ %>
        <div class="timeline-row active">
            <div class="timeline-time">
                <small><%= data[i].time.split(" ")[0] %></small>
                <%= data[i].time.split(" ")[1] %>
            </div>
            <div class="timeline-icon"></div>
            <div class="panel timeline-content">
                <div class="panel-body">
                    <h2><%= data[i].title %></h2>
                    <a href="/upload/<%= data[i].img %>.jpg" target="_blank"><img class="img-responsive"
                            src="/upload/<%= data[i].img %>.jpg"></a>
                    <p><%= data[i].text %></p>
                </div>
            </div>
        </div>
        <% } %>
    </div>
    <script type="text/javascript" src="/static/jquery-1.10.2.min.js"></script>
    <script type="text/javascript" src="/static/bootstrap.min.js"></script>
    <script type="text/javascript" src="/static/main.js"></script>
</body>
</html>